[Unit]
Description=EdgeOS Device UUID Generation
DefaultDependencies=no
After=local-fs.target data.mount edgeos-etc-binds.service
Wants=edgeos-etc-binds.service

[Service]
Type=oneshot
ExecStart=/usr/bin/generate-uuid.sh
RemainAfterExit=yes
StandardOutput=journal
StandardError=journal

[Install]
WantedBy=sysinit.target
