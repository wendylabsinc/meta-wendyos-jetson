[connection]
id=usb-gadget
uuid=12345678-1234-1234-1234-123456789abc
type=ethernet
interface-name=usb0
autoconnect=true
autoconnect-priority=100
wait-device-timeout=60000

[ethernet]
mac-address-blacklist=

[ipv4]
method=auto
dns-search=
# Uses DHCP to get IP from host computer's internet sharing

[ipv6]
method=link-local
addr-gen-mode=stable-privacy

[proxy]
